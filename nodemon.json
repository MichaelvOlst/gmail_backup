{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "./gmail_backup",
    "./gmail_backup.db",
    "./go.mod",
    "./go.sum",
    ".vscode",
    "public",
    "resources",
    "*-packr.go"
  ],
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "go": "go"
  },
  "ext": "go,json,js,vue"
}